# git checkout
alias gco='git checkout - '

# git commit
alias gc='git commit -m '
alias gcma='git commit -a -m'

# git config
alias egc='git config -e'
alias egcg='git config --global -e'
alias gclso='git config --list --show-origin'

# git diff
alias gd='git diff '

